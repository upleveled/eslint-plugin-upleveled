{
  "name": "eslint-plugin-upleveled",
  "version": "2.1.15",
  "repository": "upleveled/eslint-plugin-upleveled",
  "license": "MIT",
  "author": "UpLeveled (https://github.com/upleveled)",
  "contributors": [
    "José Fernando Höwer Barbosa <josehowerupn@gmail.com>",
    "Karl Horky <karl.horky@gmail.com>"
  ],
  "type": "module",
  "main": "index.js",
  "files": [
    "index.js",
    "rules/*.js"
  ],
  "scripts": {
    "build": "tsc --project tsconfig.build.json",
    "prepare": "pnpm build"
  },
  "devDependencies": {
    "@types/node": "25.0.10",
    "@typescript-eslint/types": "8.53.1",
    "@typescript-eslint/typescript-estree": "8.53.1",
    "@typescript-eslint/utils": "8.53.1",
    "eslint-config-upleveled": "9.4.4",
    "eslint-plugin-upleveled": "2.1.14",
    "prettier": "3.8.1",
    "typescript": "5.9.3"
  },
  "peerDependencies": {
    "eslint": "^9.39.2"
  },
  "packageManager": "pnpm@10.28.1",
  "engines": {
    "node": ">=18.0.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "unrs-resolver"
    ],
    "overrides": {
      "@typescript-eslint/types": "$@typescript-eslint/types",
      "@typescript-eslint/utils": "$@typescript-eslint/utils"
    }
  }
}
